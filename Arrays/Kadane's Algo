TC: O(n)
SC: O(1)
class Solution {
    public int maxSubArray(int[] nums) {
        int maxi = 0;
        int sum = 0;
        for(int i=0;i<nums.length;i++){
            sum=sum+nums[i];
            if(sum>maxi){
                maxi=sum;
            }
            if(sum<0){
                sum=0;
            }
            maxi=Math.max(sum,maxi);
        }
        return maxi;
    }
}
